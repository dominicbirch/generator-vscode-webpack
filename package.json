{
  "name": "generator-vscode-webpack",
  "version": "1.0.0",
  "description": "Generator for projects creating typescript vscode extensions using webpack",
  "homepage": "https://github.com/dominicbirch/generator-vscode-webpack",
  "author": {
    "name": "Dominic Birch",
    "email": "dominicbirch@outlook.com",
    "url": "https://github.com/dominicbirch"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "typescript",
    "webpack",
    "npm",
    "package",
    "module",
    "yeoman-generator"
  ],
  "peerDependencies": {
    "yeoman-generator": "^5.4.2"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "xo": "^0.44.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,ts}": "xo --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "xo": {
    "envs": [
      "es2021",
      "node",
      "jest"
    ],
    "prettier": true
  },
  "scripts": {
    "lint": "xo --fix",
    "test": "jest"
